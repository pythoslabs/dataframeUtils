def filter_df(df_in,query) :
    t1 = time.time()
    df_out=df_in.query(query)
    t2 = time.time()
    print("Time taken = " , (t2-t1) , " seconds")
    #filtered_df.to_csv(output_file_name, encoding='utf-8', index=None)
    return df_out
    
#usage
df_filtered= filter_df(train_head,'target==1')    
